  "scripts": {
    "ng": "ng",
    "prodstart": "node ./server.js",
    "build:client": "ng build --prod --output-path ./backend/views --aot --buildOptimizer --commonChunk --vendorChunk --optimization --progress",
    "test:client": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "echo \"Error: no test specified\"",   
    "startserver": "nodemon ./backend/server.js",
    "buildclient": "ng build",
    "runclient": "ng serve --proxy-config proxy.conf.json",
    "devstart": "nodemon --inspect ./backend/server.js",
    "dev": "npm run buildclient & concurrently --kill-others \"npm run runclient\" \"npm run devstart\"",
    "startapp": "concurrently --kill-others \"npm run runclient\" \"npm run devstart\"",
    "start": "node ./bin/www",
    "start2": "nodemon ./server.js",
    "start_azure_debug": "node --inspect=0.0.0.0:$APPSVC_TUNNEL_PORT app.js"  
  },
  "private": true,
  "dependencies": {

    "@azure/msal-node": "^1.0.0-beta.0",
