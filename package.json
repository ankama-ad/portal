    "adal-node": "^0.2.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "concurrently": "^6.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "guid": "0.0.12",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "powerbi-client": "^2.16.5",
    "sequelize": "^6.6.2",
